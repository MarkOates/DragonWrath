

#include <DragonWrath/Worlds/Base.hpp>


namespace DragonWrath
{
namespace Worlds
{


Base::Base(std::string title, std::vector<std::string> levels_to_load)
   : title(title)
   , levels_to_load({})
   , current_level_index_num(0)
{
}


Base::~Base()
{
}


std::string Base::get_title()
{
}


bool Base::next_level_exists()
{
   return current_level_index_num == levels_to_load.size();
}



} // namespace Worlds
} // namespace DragonWrath


